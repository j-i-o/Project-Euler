public class Uno{

     public static void main(String []args){
        System.out.println("Suma de multiplos de 3 y 5");
        int sumatoria = 0;
        for (int i = 1; i < 1000; i++){
            if(i % 3 == 0 || i % 5 == 0){
                System.out.println(i);
                sumatoria += i;
            }
        }
        System.out.println("Sumatoria total: " + sumatoria);
     }
}
